{{!-- pages/partners.hbs --}}

{{#> layouts/base title="Page Two" }}

  {{!-- Let's add a second stylesheet for this layout. --}}
  {{#*inline "head-block"}}
    <link rel="stylesheet" href="css/app.css">
  {{/inline}}

  {{!-- Let's change the header image for this layout. --}}
  {{#*inline "header-block"}}
    {{> includes/header
      header-src="http://fpoimg.com/400x400"
      header-alt="header 2 alt title"
    }}
  {{/inline}}

  {{#*inline "sidebar-block"}}
      {{> includes/sidebar}}
  {{/inline}}

  {{!-- Let's override the "footer-block" content. --}}
  {{#*inline "footer-block"}}
    <p>We are now overriding the default "footer-block" content with this content.</p>
  {{/inline}}

  {{!-- Let's add a script for this layout. --}}
  {{#*inline "scripts-block"}}
    <script src="../.././js/main.js"></script>
  {{/inline}}

{{/layouts/base}}
